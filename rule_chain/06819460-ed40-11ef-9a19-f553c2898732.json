      "configuration" : {
        "scriptLang" : "TBEL",
        "jsScript" : "return {msg: msg, metadata: metadata, msgType: msgType};",
        "tbelScript" : "// JS nonsense for n decimal digit formatting is to use Math.round.\n// Luckily, TBEL has toFixed method useful for rounding and formatting.\n\n// Note:\n// toFixed(10*value)/1000.0\n// Is equivalent to scaling by 0.01 rounded to 3 digits\n\nvar names, nm;\n\n// Wind speed: scale to knots and 2 decimal digits.\nnames = [\"Scalar Wind Speed\", \"Gust Wind Speed\"];\nforeach (n : names) {\n    if (msg[n] !=null) {\n        msg[n] = toFixed(1.943844*msg[n], 2);\n    }\n}\n\n// Direction: round to 10 degrees are requrested\nnames = [ \"Scalar Wind Direction\", \"Gust Wind Direction\"];\nforeach ( n : names ) {\n    if (msg[n]!=null) { \n        msg[n] = toFixed( toFixed( msg[n]/10.0, 0 )*10, 0);\n    }\n}\n\n// Round to 1 decimal digit \nnames = [\"Air Temperature\", \"Relative Humidity\", \"Rain\", \"Rainfall\", \"Direct Solar Radiation\"];\nforeach (n : names) {\n    if (msg[n]!=null) {\n        msg[n] = toFixed( msg[n], 1);\n    }\n}\n\n\n// Scale pressure to millibars/hPa\nnm=\"Atmospheric Pressure\";\nif (msg[nm]!=null) {\n    msg[nm] = toFixed(msg[nm]/100.0, 2);\n}\n\nreturn {msg: msg, metadata: metadata, msgType: msgType};"
      },
